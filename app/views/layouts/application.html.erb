<!DOCTYPE html>
<html>
	<head>
	
	<TITLE><%= title %> </TITLE>
	  <%= csrf_meta_tags %>
	  <%= render 'layouts/stylesheets' %>
	</head>
	
	<body>
		<% flash.each do |key, value| %>
				<p><%= value %></p>
		<% end %>
		<div class="container">
			<%= render 'layouts/header' %>
			<section class="round">
			<%= yield %>
			</section>
			<%= render 'layouts/footer' %>
			<%= debug(params) if Rails.env.development?%>
		</div>

	</body>
</html>
